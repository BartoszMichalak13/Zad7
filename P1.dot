zwierzeta [shape = record, label = "{ zwierzeta | + zwierzeta() : \l|+ wypiszzwierzeta() : void\l }"];

ptaki     -> zwierzeta [color = "#000082", style = solid , arrowtail = none    , arrowhead = normal  , taillabel = "", label = "", headlabel = ""];
